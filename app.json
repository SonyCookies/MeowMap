{
  "expo": {
    "name": "MeowMap",
    "slug": "MeowMap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.meowmap.app",
      "userInterfaceStyle": "light",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to show your position on the map and nearby cat sightings.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We use your location to show your position on the map and nearby cat sightings."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "meowmap"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.meowmap.app"
    },
    "scheme": "meowmap",
    "plugins": [
      [
        "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1Ijoic29ubnlzYXJjaWEiLCJhIjoiY21qam4zcmp2MDB1dDNrc2RrZ3RzNGZmZiJ9.jOkWhS55K8XqFpUjc3kHEA"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d1dba4e4-e7f1-48b6-9742-bcdf9572e34c"
      }
    }
  }
}
